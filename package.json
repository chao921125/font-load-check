{
  "name": "font-load-checker",
  "version": "1.1.6",
  "description": "一个用于检测、管理和操作字体的现代 JavaScript/TypeScript 库，基于最新的 Web Font API",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "unpkg": "dist/index.umd.js",
  "jsdelivr": "dist/index.umd.js",
  "files": [
    "dist",
    "docs",
    "examples",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean"
  },
  "keywords": [
    "font",
    "font-loading",
    "font-check",
    "font-face",
    "css-font-loading-api",
    "web-fonts",
    "font-management",
    "typescript",
    "javascript"
  ],
  "author": {
    "name": "huangchao",
    "email": "chao921125@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/huangchao/font-load-checker.git"
  },
  "bugs": {
    "url": "https://github.com/huangchao/font-load-checker/issues"
  },
  "homepage": "https://github.com/huangchao/font-load-checker#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.2",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-typescript": "^11.1.1",
    "@types/jest": "^29.5.2",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "rollup": "^3.25.1",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^29.1.0",
    "tslib": "^2.5.3",
    "typescript": "^5.1.3"
  },
  "publishConfig": {
    "access": "public"
  }
